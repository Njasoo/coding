template <typename T>
struct Fenwick {
  int n;
  vector<T> t;
  Fenwick(int _n) : n(_n), t(_n + 1) {}
  int lowbit(int x) { return x & -x; }
  void add(int x, T k) {
    while (x <= n) {
      t[x] += k;
      x += lowbit(x);
    }
  }
  T sum(int x) {
    T res = 0;
    while (x) {
      res += t[x];
      x -= lowbit(x);
    }
    return res;
  }
};