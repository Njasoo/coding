template <typename T>
struct heap {
  priority_queue<T> pque, temp;
  bool empty() { get(); return pque.empty(); }
  void push(T x) { pque.push(x); }
  T get() {
    while (!pque.empty() && !temp.empty() && pque.top() == temp.top()) {
      pque.pop();
      temp.pop();
    }
    return pque.top();
  }
  void erase(T x) { temp.push(x); }
};
