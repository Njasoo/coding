int st[N][21];
const int LOG = 21;
void build() {
  for (int i = 1; i <= n; i++) {
    st[i][0] = b[i];
  }
  for (int j = 1; j < LOG; j++) {
    for (int i = 1; i + (1 << j) - 1 <= n; i++) {
      st[i][j] = std::max(st[i][j - 1], st[i + (1 << (j - 1))][j - 1]);
    }
  }
}
int query(int l, int r) {
  int k = (int) log2(r - l + 1);
  return std::max(st[l][k], st[r - (1 << k) + 1][k]);
}
